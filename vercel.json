{
  "version": 2,
  "builds": [
    {
      "src": "klasifikasi.py",
      "use": "@vercel/python",
      "config": {
           "maxLambdaSize": "15mb",
           "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "klasifikasi.py"
    }
  ],
   "buildCommand": "pip install -r requirements.txt -t .vercel/output/function",
     "outputDirectory": ".vercel/output"
}
